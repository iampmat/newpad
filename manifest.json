{
  "manifest_version": 2,

  "name": "<new pad>",
  "description": "This extension creates a note pad in a new tab",
  "version": "1.0",

  "permissions": [
    "tabs",
    "topSites"
  ],
  "browser_action": {
    
  },
  "chrome_url_overrides": {
    "newtab": "newpad.html"
  },
  "icons": { "16": "newpadicon16.png",
           "48": "newpadicon48.png",
          "128": "newpadicon128.png" }
}